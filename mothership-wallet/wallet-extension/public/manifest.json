{
  "manifest_version": 3,
  "name": "Mothership Wallet",
  "version": "0.3.0",
  "action": {
    "default_title": "Mothership Wallet",
    "default_popup": "index.html"
  },
  "icons": {
    "128": "alien-128.png",
    "256": "alien-256.png",
    "512": "alien-512.png"
  },
  "permissions": [
    "storage",
    "unlimitedStorage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}